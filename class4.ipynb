    "\n",
    "class solution:\n",
    "    def twosum(self,nums,target):\n",
    "        d={}\n",
    "        for i,num in enumerate(nums):\n",
    "            if target-num in d:\n",
    "                return (d[target-num],i)\n",
    "            d[num]=i\n",
    "print(\"index1= %d, index2= %d\"%solution().twosum([90,20,10,40,50,60,70],50))"
   ]
